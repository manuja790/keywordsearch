<jsp:useBean id="obj1" class="keyword.FindKNN"/>
<%@page import="java.sql.*"%>
<%@page import="java.util.ArrayList"%>
<%

	String a=null;
  String b=null;	
ResultSet rs=obj.Findefg();
		try {
			while(rs.next())
			{
				a=rs.getString(2);
				b=rs.getString(3);
				
			}
